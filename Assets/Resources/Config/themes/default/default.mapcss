canvas { 
	z-index: 0; 
	height: 10;
	
	pixelMapError: 1;
	resolution: 512;
	heightmapsize: 513;
	
	splat: 0, Asphalt_background, 5, 5;
	splat: 1, Grass, 5, 5;
	splat: 2, Pebbles, 5, 5;
	
	detail: 0, Weed2;
}
/* flat areas, general rules */
area[amenity],
area[natural],
area[historic=memorial],
area[landuse]											{ builder: splat; splat: 2; z-index: 10; fill-color: #FF0000;}


/* buildings */
/* Take note that building-style is set to residential. We have to distinguish different types */
/*area[barrier=fence],*/
area[man_made=tower],
area[building:levels],
area[building:shape],
area[building:part],
area[building]											{ z-index: 10; builder: building; behaviour: solid; height:12; min_height: 0; building-style:residential; fill-color:#f4f4f4; roof-color:#a5a5a5; roof-style:flat;}

area[amenity=shelter][shelter_type=public_transport]						{z-index: 9;  builder: building; behaviour: solid; height:3; building-style:residential; fill-color:#f4f4f4; roof-color:#a5a5a5; roof-style:flat;}

/* TODO: Dangerous rules! Sometimes builder is missed but this rule is applied */

/* some of the buildings have set layer=1 instead of min_height*/
/*area[building][layer>0]								{ min_height: eval(num(tag('layer')) * num(3));}*/

/*area[building:levels]									{ height: eval(num(tag('building:levels')) * num(5));}*/
area[height]												{ height: eval(num(tag('height')));}
area[building:height]									{ height: eval(num(tag('building:height')));}
area[building:height][roof:height]						{ height: eval(num(tag('building:height')) - num(tag('roof:height')));}

area[building:min_height]								{ min_height: eval(num(tag('building:min_height')));}
area[min_height]											{ min_height: eval(num(tag('min_height')));}

area[building=residential], 	
area[building=yes]										{ builder: building; building-style:residential;}
area[building:part],area[building=yes]					{ part: true; building-style: part;}	

area[building:shape=sphere]								{ builder: sphere; material: Buildings/Building;}
area[building:shape=cylinder]							{ builder: cylinder; material: Buildings/Building;}					


area[building=bridge]									{height:0.1;}
